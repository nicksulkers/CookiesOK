{
	"options_page": "pages/options/index.html",
	"author": "Nick Sulkers",
	"background": {
		"scripts": [
			"scripts/background.js"
		]
	},
	"default_locale": "en",
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		},
		"default_title": "CookiesOK",
		"default_popup": "pages/action/index.html"
	},
	"content_scripts": [
		{
			"js": [
				"scripts/pre.js"
			],
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_start"
		},
		{
			"js": [
				"scripts/cookiesok.js"
			],
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_idle"
		}
	],
	"description": "CookiesOK aims to automatically accept cookies on websites that are affected by the EU cookie laws.",
	"homepage_url": "https://cookiesok.com",
	"icons": {
		"16": "images/icon16.png",
		"19": "images/icon19.png",
		"32": "images/icon32.png",
		"38": "images/icon38.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 2,
	"name": "CookiesOK",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"contextMenus",
		"tabs"
	],
	"version": "5.2"
}